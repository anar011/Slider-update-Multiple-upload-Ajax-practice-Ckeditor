@model ExpertHeader


@{
    ViewData["Title"] = "Expert";
}



<div class="container my-5">
    <span class="text-danger">@ViewBag.error</span>
    <form class="forms-sample" asp-action="Create">
        <input type="text" class="form-control" id="customFile" asp-for="Title" />     
        <span class="text-danger" asp-validation-for="Title"></span>
        <input type="text" class="form-control" id="customFile" asp-for="Description" />
        <span class="text-danger" asp-validation-for="Description"></span>
        <div class="button my-3">
            <button type="submit" class="btn btn-primary mr-2">Submit</button>
            <a class="btn btn-light" asp-action="Index">Cancel</a>
        </div>

    </form>
</div>
